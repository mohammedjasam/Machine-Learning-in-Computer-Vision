function[acc, comp] = checkAccuracy(DistanceMatrix, Indices, NumTestImages, TrainingLabels, TestingLabels)
    
    count = 0;
    comp = zeros(NumTestImages, 2);
    
    for i = 1 : NumTestImages
        comp(i,:) = [TestingLabels(i,1), TrainingLabels(Indices(i),1)];
        if(TestingLabels(i,1) == TrainingLabels(Indices(i),1))
            count = count +1;
        end
    end

    %Accuracy
    acc = count/NumTestImages;
end